<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="twitterBot.TwitterAuthMapper">

    <select id="selectOne" resultType="TwitterAuthDomain">
        SELECT * FROM twitter_auth
        WHERE
            is_deleted = FALSE
    </select>

</mapper>